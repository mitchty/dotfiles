;; All these variables control what/how readme.org gets tangled
;; into dotfiles. Off by default set to t to turn them on as
;; needed.

;; OS types
(setq bsd-p nil)
(setq linux-p nil)
(setq osx-p nil)

;; Optional things
(setq nix-p nil)
(setq tmux-p nil)
(setq git-p nil)
(setq emacs-p nil)
(setq vim-p nil)
(setq zsh-p nil)
(setq mosh-p nil)
(setq x-p nil)

;; Language specific
(setq haskell-p nil)
(setq perl-p nil)
(setq ruby-p nil)
